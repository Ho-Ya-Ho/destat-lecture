{
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "framework": null,
  "rewrites": [
    {
      "source": "/((?!assets/).*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "includeFiles": "build/server/**"
    }
  }
}

